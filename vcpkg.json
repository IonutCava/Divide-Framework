{
    "name": "divide-framework",
    "version-string": "latest",
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        "pkgconf",
        "boost-property-tree",
        "boost-asio",
        "stb",
        "freetype",
        "fmt",
        "concurrentqueue",
        "devil",
        "eastl",
        "simpleini",
        "glbinding",
        "glm",
        "meshoptimizer",
        "nvtt",
        "openal-soft",
        "expat",
        "recastnavigation",
        "imgui-node-editor",
        "vulkan",
        "vulkan-memory-allocator",
        "vk-bootstrap",
        "glslang",
        "ctre",
        "catch2",
        "fribidi",
        "assimp",
        "sdl2",
        "sdl2-image",
        "mimalloc",
        "physx",
        {
            "name": "sdl2-mixer",
            "features": [
                "fluidsynth",
                "libflac",
                "libmodplug",
                "mpg123",
                "wavpack",
                "opusfile"
            ]
        },
        {
            "name": "imgui",
            "features": [
                "docking-experimental",
                "no-obsolete-keyio",
                "no-obsolete-functions",
                "sdl2-binding",
                "sdl2-renderer-binding"
            ]
        }
    ]
}